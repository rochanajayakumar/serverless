{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "dueDate": {
      "type": "string"
    },
    "todoId": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "createdAt":{
      "type": "string"
    },
    "done":{
      "type": "boolean"
    },
    "attachmentUrl":{
      "type": "string"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}